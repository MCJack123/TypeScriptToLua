{
    "compilerOptions": {
        "outDir": "../../dist/lualib",
        "target": "esnext",
        "lib": ["esnext"],
        "types": ["@jackmacwindows/lua-types/cc"],
        "skipLibCheck": true,

        "noUnusedLocals": true,
        "noUnusedParameters": true
    },
    "tstl": {
        "luaLibImport": "none",
        "noHeader": true,
        "luaPlugins": [{ "name": "../../dist/lualib-build/plugin.js" }]
    },
    "include": [".", "../../language-extensions/index.d.ts"]
}
